{
  "manifest_version": 3,
  "name": "Hello Extensions ss",
  "description": "Base Level Extension",
  "version": "1.0",
  "options_page": "options.html",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content-github.js"
      ],
      "matches": [
        "https://github.com/*"
      ]
    }
  ],
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": [
    "storage",
    "contextMenus"
  ]
}
